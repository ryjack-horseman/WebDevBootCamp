<%- include("partials/header") -%>
<h1>Home</h1>
<p><%=  startingContent%></p>
<% posts.forEach(function(post){ %>
    <div class="post">
        <h1><%= post.postTitle %></h1>
        <p><%= post.postContent.substring(0, 100) + "..." %>
        <a href="/posts/<%= post.postTitle %>">Read More</a></p>
    </div>
<% }) %>
<%- include("partials/footer") -%>